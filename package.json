{
  "name": "reactive-di",
  "version": "0.0.1",
  "description": "Reactive dependency injection",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "main": "dist/index.js",
  "options": {
    "builddir": "dist"
  },
  "scripts": {
    "postinstall": "test -d conf && conf/resources/postinstall.sh $npm_package_name || exit 0",
    "prepush": "exit 0 && npm test",
    "prepublish": "conf/resources/prepublish.sh",
    "preversion": "conf/resources/checkgit.sh && npm test",
    "postversion": "git push && git push --tags",
    "precommit": "exit 0",
    "update": "ncu -ua && npm install",
    "clean": "rm -rf $npm_package_options_builddir coverage",
    "build": "npm run clean && babel src --source-maps --out-dir $npm_package_options_builddir",
    "build.dev": "npm run build -- --watch",
    "lint": "exit 0 && eslint src",
    "check": "flow check",
    "testonly": "mocha",
    "test": "npm run lint && npm run check && npm run testonly",
    "test.dev": "npm run testonly -- --growl --watch",
    "test.cov": "babel-istanbul cover --report text --report html node_modules/mocha/bin/_mocha"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/reactive-di.git"
  },
  "keywords": [
    "reactive-di",
    "dependency injection",
    "di",
    "modular",
    "state",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "flux",
    "elm"
  ],
  "files": [
    "*.map",
    "*.js",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "devDependencies": {
    "babel-runtime": "5.x.x",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-espower": "^2.1.1",
    "babel-plugin-flow-comments": "^6.3.19",
    "babel-plugin-syntax-decorators": "^6.3.13",
    "babel-plugin-syntax-flow": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.4.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "4.0.0",
    "eslint-config-airplus": "^1.0.6",
    "eslint-plugin-react": "^3.16.1",
    "glob": "^6.0.4",
    "mocha": "^2.4.5",
    "power-assert": "^1.2.0",
    "sinon": "^1.17.3"
  },
  "dependencies": {}
}
