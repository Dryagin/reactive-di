{
    "presets": ["es2015-loose", "stage-0", "react"],
    "plugins": [
        ["module-resolver", {
            "root": [""],
            "alias": {
                "reactive-di": "./src"
            }
        }],
        "syntax-flow",
        "espower",
        ["transform-react-jsx", {
            "pragma": "_h"
        }],
        "transform-decorators-legacy",
        "transform-flow-strip-types"
    ]
}
